            <hr>
        </div>
        <div class="col-sm-4 col-sm-offset-1">
            <figure>
                <img src="~/Content/Imagenes/error.svg">
            </figure>
            <h3>Otras opciones:</h3>
            <ul>
                <li><a href="@Url.Action("InicioInversionista","Home")">Regresar al inicio</a></li>
                <li><a href="http://www.gob.mx/busqueda">Buscar en gob.mx</a></li>
                <li><a href="javascript:history.back();">Volver a la página anterior</a></li>
            </ul>
        </div>
    </div>
</div>