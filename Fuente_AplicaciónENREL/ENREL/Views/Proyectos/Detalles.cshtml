@model ENREL.Models.Proyectos.CatProyectos
@{
    ViewBag.Title = "Detalles Proyecto";
    string NombreUsuarioSession = "";
    if (Session["NombreUsuario"] != null)
    {
        NombreUsuarioSession = Session["NombreUsuario"].ToString();
    }
}

<!-- Begin Digital Analytix Tag 1.1302.13 -->
<script type="text/javascript">
    udm_('https://sb.scorecardresearch.com/b?c1=2&c2=17183199&ns_site=gobmx&name=SENER.ENREL.INVERSIONISTA.PROYECTOS.DETALLE');
</script>
<noscript><p><img src="'https://sb.scorecardresearch.com/b?c1=2&c2=17183199&ns_site=gobmx&id_usuario=' + @NombreUsuarioSession + '&name=SENER.ENREL.INVERSIONISTA.PROYECTOS.DETALLE" height="1" width="1" alt="*"></p></noscript>
<!-- End Digital Analytix Tag 1.1302.13 -->

@*Breadcum: *@
<div class="row" style="margin-top:94px">
    <div class="col-sm-8">
        <ol class="breadcrumb">
            <li><a href="https://www.gob.mx/" target="_blank"><i class="icon icon-home"></i></a></li>
            <li><a href="@Url.Action("InicioInversionista","Home")">Inicio</a></li>
            <li><a href="@Url.Action("Index","Proyectos")">Proyectos</a></li>
            <li class="active">
                Detalles de proyectos
            </li>
        </ol>
    </div>
    <div class="col-sm-4">
        <div class="user-credencials">
            <ul class="list-unstyled">
                <li>
                    <span class="user-credencials__name">@NombreUsuarioSession</span>
                    <button type="button" class="btn btn-link pull-right" onclick="javascript: uid_call('SENER.ENREL.INVERSIONISTA.BTN_CERRARSESIÓN', 'clickout'); location.href = '@Url.Action("Logout", "Home")'">Salir</button>
                </li>
                <li><a href="@Url.Action("Actualizar", "Usuarios")">Modificar usuario</a></li>
            </ul>
        </div>
    </div>
</div>



@*Título y descripción: *@
<h3>Información del proyecto </h3>
<div class="vertical-buffer"></div>

@*Contenido: *@
<table class="table" style="width:50%;">

    <tbody>
        <tr>
            <td>
                @Html.Label("Nombre")
            </td>
            <td>
                @Html.DisplayFor(pro => pro.P_NombreProyecto)
            </td>
        </tr>
        <tr>
            <td>
                @Html.Label("Empresa")
            </td>
            <td>
                @Html.DisplayFor(pro => pro.P_Empresa)
            </td>
        </tr>
        <tr>
            <td>
                @Html.Label("Tecnología")
            </td>
            <td>
                @Html.DisplayFor(pro => pro.P_Tecnologia)
            </td>
        </tr>
        <tr>
            <td>
                @Html.Label("Municipio o Alcaldía")
            </td>
            <td>
                @Html.DisplayFor(pro => pro.P_Municipio)
            </td>
        </tr>
        <tr>
            <td>
                @Html.Label("Latitud")
            </td>
            <td>
                @Html.DisplayFor(pro => pro.P_Latitud)
            </td>
        </tr>
        <tr>
            <td>
                @Html.Label("Longitud")
            </td>
            <td>
                @Html.DisplayFor(pro => pro.P_Longitud)
            </td>
        </tr>
        <tr>
            <td>
                @Html.Label("Capacidad instalada")
            </td>
            <td>
                @Html.DisplayFor(pro => pro.P_CapacidadInstalada) MW
            </td>
        </tr>
        <tr>
            <td>
                @Html.Label("Generación anual")
            </td>
            <td>
                @Html.DisplayFor(pro => pro.P_GeneracionAnual) GWh
            </td>
        </tr>
        <tr>
            <td>
                @Html.Label("Factor planta")
            </td>
            <td>
                @Html.DisplayFor(pro => pro.P_FactorPlanta)
            </td>
        </tr>
        <tr>
            <td>
                @Html.Label("Monto de inversión")
            </td>
            <td>
                @Html.DisplayFor(pro => pro.P_MontoInversion) USD
            </td>
        </tr>
        <tr>
            <td>
                @Html.Label("Fecha de registro")
            </td>
            <td>
                @Html.DisplayFor(pro => pro.P_FechaRegistro)
            </td>
        </tr>
    </tbody>
</table>

